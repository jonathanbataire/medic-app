<?xml version="1.0"?>
<h:html xmlns="http://www.w3.org/2002/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:jr="http://openrosa.org/javarosa" xmlns:orx="http://openrosa.org/xforms" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <h:head>
    <h:title>Add Family</h:title>
    <model>
      <instance>
        <data delimiter="#" id="contact:clinic:create" prefix="J1!contact:clinic:create!" version="2018-08-16  12-56">
          <inputs>
            <meta>
              <location>
                <lat/>
                <long/>
                <error/>
                <message/>
              </location>
            </meta>
            <user>
              <facility_id/>
            </user>
          </inputs>
          <contact>
            <name/>
            <debug_note/>
            <parent>PARENT</parent>
            <sex/>
            <date_of_birth_method/>
            <date_of_birth/>
            <phone/>
            <pregnant_at_registration/>
            <care_giver/>
            <phone1/>
          </contact>
          <ephemeral_dob>
            <g_sex/>
            <contact_dob_method>approx</contact_dob_method>
            <dob_calendar/>
            <age/>
            <dob_raw/>
            <contact_dob_iso/>
          </ephemeral_dob>
          <ephemeral_pregnancy>
            <display_name/>
            <pregnant/>
            <note_p_form/>
            <lmp/>
            <lmp_start/>
            <lmp_approx/>
            <hh_fp_flow>
              <hh_fp/>
              <hh_family_planning_method/>
              <hh_fp_enroll/>
              <hh_fp_method_choice/>
              <note_fp_1/>
              <note_fp_2/>
              <note_fp_3/>
            </hh_fp_flow>
            <g_children_under_5/>
            <g_women_15_to_49/>
            <note_msg/>
          </ephemeral_pregnancy>
          <repeat>
            <child_count/>
            <child jr:template="">
              <type>person</type>
              <parent>PARENT</parent>
              <name/>
              <date_of_birth/>
              <sex/>
              <pregnant_at_registration/>
              <c_name/>
              <family_member_type/>
              <relation/>
              <dob_method>approx</dob_method>
              <dob_calendar_child/>
              <dob_calendar_adult/>
              <age_years_adult>0</age_years_adult>
              <age_years_child>0</age_years_child>
              <age_months_adult>0</age_months_adult>
              <age_months_child>0</age_months_child>
              <age_years/>
              <age_months/>
              <dob_calendar/>
              <dob_raw/>
              <c_dob_iso/>
              <current_age>0</current_age>
              <c_sex/>
              <group_other_woman_pregnancy>
                <married/>
                <other-woman-pregnant/>
                <o_lmp/>
                <o_lmp_start/>
                <o_lmp_approx/>
                <p_test/>
                <p_test_result/>
                <p_test_kit/>
                <p_test_kit_result/>
                <note_p/>
                <fp_flow>
                  <fp/>
                  <family_planning_method/>
                  <fp_enroll/>
                  <fp_method_choice/>
                  <note_fp_1/>
                  <note_fp_2/>
                  <note_fp_3/>
                </fp_flow>
              </group_other_woman_pregnancy>
            </child>
          </repeat>
          <clinic>
            <parent/>
            <name/>
            <geolocation/>
            <contact/>
            <household_survey>
              <safe_water/>
              <source_clean_water/>
              <source_of_drinking_water/>
              <kitchen/>
              <drying_rack/>
              <rubbish_pit/>
              <hygeinic_toilet/>
              <mosquito_nets/>
            </household_survey>
          </clinic>
          <meta tag="hidden">
            <instanceID/>
          </meta>
        </data>
      </instance>
      <bind nodeset="/data/inputs" relevant="0"/>
      <bind nodeset="/data/inputs/user/facility_id" type="string"/>
      <bind calculate=" /data/contact/care_giver " nodeset="/data/contact/name" required="true()" type="string"/>
      <bind nodeset="/data/contact/debug_note" readonly="true()" relevant="0" type="string"/>
      <bind nodeset="/data/contact/parent" type="string"/>
      <bind calculate=" /data/ephemeral_dob/g_sex " nodeset="/data/contact/sex" type="string"/>
      <bind calculate=" /data/ephemeral_dob/contact_dob_method " nodeset="/data/contact/date_of_birth_method" type="string"/>
      <bind calculate=" /data/ephemeral_dob/contact_dob_iso " nodeset="/data/contact/date_of_birth" type="string"/>
      <bind calculate=" /data/contact/phone1 " nodeset="/data/contact/phone" type="string"/>
      <bind calculate=" /data/ephemeral_pregnancy/pregnant ='true' or  /data/repeat/child/group_other_woman_pregnancy/p_test_result ='pos' or  /data/repeat/child/group_other_woman_pregnancy/p_test_kit_result ='pos'" nodeset="/data/contact/pregnant_at_registration" type="string"/>
      <bind nodeset="/data/contact/care_giver" required="true()" type="string"/>
      <bind jr:constraintMsg="Use one of the following formats: +256 7XX XXX XXX or 07XX XXX XXX." nodeset="/data/contact/phone1" type="tel"/>
      <bind nodeset="/data/ephemeral_dob/g_sex" required="true()" type="select1"/>
      <bind nodeset="/data/ephemeral_dob/contact_dob_method" required="true()" type="select1"/>
      <bind constraint=".&lt;today()" jr:constraintMsg="Date must be before today" nodeset="/data/ephemeral_dob/dob_calendar" relevant="selected(../contact_dob_method,'calendar')" required="true()" type="date"/>
      <bind constraint=". &gt;= 12 and . &lt; 120" jr:constraintMsg="Age must be between 12 and 120" nodeset="/data/ephemeral_dob/age" relevant="selected(../contact_dob_method,'approx')" required="true()" type="int"/>
      <bind calculate="if(selected( ../contact_dob_method,'calendar'), 
../dob_calendar,
format-date-time( decimal-date-time( today() ) - (365.25 * ../age) , &quot;%Y-%m-%d&quot;) )" nodeset="/data/ephemeral_dob/dob_raw" type="string"/>
      <bind calculate="format-date-time(../dob_raw,&quot;%Y-%m-%d&quot;)" nodeset="/data/ephemeral_dob/contact_dob_iso" type="string"/>
      <bind calculate=" /data/contact/care_giver " nodeset="/data/ephemeral_pregnancy/display_name" type="string"/>
      <bind nodeset="/data/ephemeral_pregnancy/pregnant" relevant=" /data/ephemeral_dob/g_sex  = 'female'" required="true()" type="select1"/>
      <bind nodeset="/data/ephemeral_pregnancy/note_p_form" readonly="true()" relevant="selected( /data/ephemeral_pregnancy/pregnant , 'true')" type="string"/>
      <bind nodeset="/data/ephemeral_pregnancy/lmp" relevant="selected( /data/ephemeral_pregnancy/pregnant , 'false')" required="true()" type="select1"/>
      <bind constraint=". &lt; today()" nodeset="/data/ephemeral_pregnancy/lmp_start" relevant="selected( /data/ephemeral_pregnancy/lmp , 'true')" type="date"/>
      <bind nodeset="/data/ephemeral_pregnancy/lmp_approx" relevant="selected( /data/ephemeral_pregnancy/lmp , 'false')" required="true()" type="select1"/>
      <bind nodeset="/data/ephemeral_pregnancy/hh_fp_flow" relevant="selected( /data/ephemeral_pregnancy/pregnant , 'false')"/>
      <bind nodeset="/data/ephemeral_pregnancy/hh_fp_flow/hh_fp" required="true()" type="select1"/>
      <bind constraint="not(selected(., 'fp_none') and count-selected(.)&gt;=2) and count-selected(.)&lt;=2 and not(count-selected(.)=2 and not(selected(., 'condoms') ))" jr:constraintMsg="None cannot be selected along with the other options. 
Condoms are the only family planing method that can be combined with another family planning method" nodeset="/data/ephemeral_pregnancy/hh_fp_flow/hh_family_planning_method" relevant="selected( /data/ephemeral_pregnancy/hh_fp_flow/hh_fp , 'true')" required="true()" type="select"/>
      <bind nodeset="/data/ephemeral_pregnancy/hh_fp_flow/hh_fp_enroll" relevant="selected( /data/ephemeral_pregnancy/hh_fp_flow/hh_fp , 'false') or selected( /data/ephemeral_pregnancy/hh_fp_flow/hh_family_planning_method , 'fp_none')" required="true()" type="select1"/>
      <bind constraint="count-selected(.)&lt;=2 and not(count-selected(.)=2 and not(selected(., 'condoms') ))" jr:constraintMsg="Condoms are the only family planing method that can be combined with another family planning method" nodeset="/data/ephemeral_pregnancy/hh_fp_flow/hh_fp_method_choice" relevant="selected( /data/ephemeral_pregnancy/hh_fp_flow/hh_fp_enroll , 'true')" required="true()" type="select"/>
      <bind nodeset="/data/ephemeral_pregnancy/hh_fp_flow/note_fp_1" readonly="true()" relevant="selected( /data/ephemeral_pregnancy/hh_fp_flow/hh_fp_enroll , 'true') and not(selected( /data/ephemeral_pregnancy/hh_fp_flow/hh_fp_method_choice , 'iud') or selected( /data/ephemeral_pregnancy/hh_fp_flow/hh_fp_method_choice , 'btl') or selected( /data/ephemeral_pregnancy/hh_fp_flow/hh_fp_method_choice , 'norplant'))" type="string"/>
      <bind nodeset="/data/ephemeral_pregnancy/hh_fp_flow/note_fp_2" readonly="true()" relevant="selected( /data/ephemeral_pregnancy/hh_fp_flow/hh_fp_enroll , 'true') and (selected( /data/ephemeral_pregnancy/hh_fp_flow/hh_fp_method_choice , 'iud') or selected( /data/ephemeral_pregnancy/hh_fp_flow/hh_fp_method_choice , 'btl') or selected( /data/ephemeral_pregnancy/hh_fp_flow/hh_fp_method_choice , 'norplant'))" type="string"/>
      <bind nodeset="/data/ephemeral_pregnancy/hh_fp_flow/note_fp_3" readonly="true()" relevant="selected( /data/ephemeral_pregnancy/hh_fp_flow/hh_fp , 'false') and selected( /data/ephemeral_pregnancy/hh_fp_flow/hh_fp_enroll , 'false')" type="string"/>
      <bind constraint=". &gt;= 0" jr:constraintMsg="Number of children must be greater than 0" nodeset="/data/ephemeral_pregnancy/g_children_under_5" required="true()" type="int"/>
      <bind constraint=". &gt;= 0" jr:constraintMsg="Number of women above 12 and below 52 must be greater than or equal to 0" nodeset="/data/ephemeral_pregnancy/g_women_15_to_49" required="true()" type="int"/>
      <bind nodeset="/data/ephemeral_pregnancy/note_msg" readonly="true()" relevant=" /data/ephemeral_pregnancy/g_women_15_to_49 &gt;0 or  /data/ephemeral_pregnancy/g_children_under_5 &gt;0" type="string"/>
      <bind nodeset="/data/repeat" relevant=" /data/ephemeral_pregnancy/g_women_15_to_49 &gt;0 or  /data/ephemeral_pregnancy/g_children_under_5 &gt;0"/>
      <bind calculate="number( /data/ephemeral_pregnancy/g_children_under_5 ) + number( /data/ephemeral_pregnancy/g_women_15_to_49 )" nodeset="/data/repeat/child_count" readonly="true()" type="string"/>
      <bind nodeset="/data/repeat/child/type" type="string"/>
      <bind nodeset="/data/repeat/child/parent" type="string"/>
      <bind calculate="../c_name" nodeset="/data/repeat/child/name" required="true()" type="string"/>
      <bind calculate="../c_dob_iso" nodeset="/data/repeat/child/date_of_birth" type="string"/>
      <bind calculate="../c_sex" nodeset="/data/repeat/child/sex" type="string"/>
      <bind calculate=" /data/repeat/child/group_other_woman_pregnancy/other-woman-pregnant ='true'" nodeset="/data/repeat/child/pregnant_at_registration" type="string"/>
      <bind nodeset="/data/repeat/child/c_name" type="string"/>
      <bind nodeset="/data/repeat/child/family_member_type" required="true()" type="select1"/>
      <bind nodeset="/data/repeat/child/relation" relevant=" /data/repeat/child/family_member_type ='member_eligible_woman'" required="true()" type="select1"/>
      <bind nodeset="/data/repeat/child/dob_method" required="true()" type="select1"/>
      <bind constraint=".&lt;today() and floor(difference-in-months(., today() ) div 12 ) &lt;= 5" jr:constraintMsg="Date must be before today and not more than 5 years ago." nodeset="/data/repeat/child/dob_calendar_child" relevant="selected(../dob_method,'calendar') and  /data/repeat/child/family_member_type ='member_child'" required="true()" type="date"/>
      <bind constraint=".&lt;today() and floor(difference-in-months(., today() ) div 12 ) &gt; 5" jr:constraintMsg="Date must be before today and more than 5 years ago." nodeset="/data/repeat/child/dob_calendar_adult" relevant="selected(../dob_method,'calendar') and  /data/repeat/child/family_member_type ='member_eligible_woman'" required="true()" type="date"/>
      <bind constraint=". &gt; 5" jr:constraintMsg="Invalid Age Value" nodeset="/data/repeat/child/age_years_adult" relevant="selected(../dob_method,'approx') and  /data/repeat/child/family_member_type ='member_eligible_woman'" required="true()" type="int"/>
      <bind constraint=". &lt; 5" jr:constraintMsg="Invalid Age Value" nodeset="/data/repeat/child/age_years_child" relevant="selected(../dob_method,'approx') and  /data/repeat/child/family_member_type ='member_child'" required="true()" type="int"/>
      <bind constraint=". &gt;= 0 and . &lt;= 11" jr:constraintMsg="Months must between 0 and 11" nodeset="/data/repeat/child/age_months_adult" relevant="selected(../dob_method,'approx') and  /data/repeat/child/family_member_type ='member_eligible_woman'" required="true()" type="int"/>
      <bind constraint=". &gt;= 0 and . &lt;= 11" jr:constraintMsg="Months must between 0 and 11" nodeset="/data/repeat/child/age_months_child" relevant="selected(../dob_method,'approx') and  /data/repeat/child/family_member_type ='member_child'" required="true()" type="int"/>
      <bind calculate="if( /data/repeat/child/family_member_type ='member_child', ../age_years_child,../age_years_adult)" nodeset="/data/repeat/child/age_years" type="string"/>
      <bind calculate="if( /data/repeat/child/family_member_type ='member_child', ../age_months_child,../age_months_adult)" nodeset="/data/repeat/child/age_months" type="string"/>
      <bind calculate="if( /data/repeat/child/family_member_type ='member_child', ../dob_calendar_child,../dob_calendar_adult)" nodeset="/data/repeat/child/dob_calendar" type="string"/>
      <bind calculate="if(not(selected( ../dob_method,'approx')), 
../dob_calendar,
format-date-time( decimal-date-time( today() ) -( (365.25 * int(../age_years)) - (30.5 * int(../age_months) )), &quot;%Y-%m-%d&quot; ) )" nodeset="/data/repeat/child/dob_raw" type="string"/>
      <bind calculate="format-date-time(../dob_raw,&quot;%Y-%m-%d&quot;)" nodeset="/data/repeat/child/c_dob_iso" type="string"/>
      <bind calculate="floor(difference-in-months(date( /data/repeat/child/c_dob_iso ),today()) div 12)" nodeset="/data/repeat/child/current_age" type="string"/>
      <bind nodeset="/data/repeat/child/c_sex" relevant=" /data/repeat/child/family_member_type ='member_child'" type="select1"/>
      <bind nodeset="/data/repeat/child/group_other_woman_pregnancy" relevant=" /data/repeat/child/family_member_type ='member_eligible_woman' and  /data/repeat/child/current_age  &gt;= 12 and  /data/repeat/child/current_age  &lt;= 52"/>
      <bind nodeset="/data/repeat/child/group_other_woman_pregnancy/married" required="true()" type="select1"/>
      <bind nodeset="/data/repeat/child/group_other_woman_pregnancy/other-woman-pregnant" type="select1"/>
      <bind nodeset="/data/repeat/child/group_other_woman_pregnancy/o_lmp" required="true()" type="select1"/>
      <bind constraint=". &lt; today()" nodeset="/data/repeat/child/group_other_woman_pregnancy/o_lmp_start" relevant="selected( /data/repeat/child/group_other_woman_pregnancy/o_lmp ,'true')" type="date"/>
      <bind nodeset="/data/repeat/child/group_other_woman_pregnancy/o_lmp_approx" relevant="selected( /data/repeat/child/group_other_woman_pregnancy/o_lmp ,'false')" required="true()" type="select1"/>
      <bind nodeset="/data/repeat/child/group_other_woman_pregnancy/p_test" relevant=" /data/repeat/child/group_other_woman_pregnancy/o_lmp_approx ='2mo' or  /data/repeat/child/group_other_woman_pregnancy/o_lmp_approx ='3mo'" required="true()" type="select1"/>
      <bind nodeset="/data/repeat/child/group_other_woman_pregnancy/p_test_result" relevant=" /data/repeat/child/group_other_woman_pregnancy/p_test ='true'" required="true()" type="select1"/>
      <bind nodeset="/data/repeat/child/group_other_woman_pregnancy/p_test_kit" relevant=" /data/repeat/child/group_other_woman_pregnancy/p_test ='false'" required="true()" type="select1"/>
      <bind nodeset="/data/repeat/child/group_other_woman_pregnancy/p_test_kit_result" relevant=" /data/repeat/child/group_other_woman_pregnancy/p_test_kit ='true'" required="true()" type="select1"/>
      <bind nodeset="/data/repeat/child/group_other_woman_pregnancy/note_p" readonly="true()" relevant=" /data/repeat/child/group_other_woman_pregnancy/other-woman-pregnant ='true' or  /data/repeat/child/group_other_woman_pregnancy/p_test_result ='pos' or  /data/repeat/child/group_other_woman_pregnancy/p_test_kit_result ='pos'" type="string"/>
      <bind nodeset="/data/repeat/child/group_other_woman_pregnancy/fp_flow" relevant=" /data/repeat/child/group_other_woman_pregnancy/other-woman-pregnant ='false'"/>
      <bind nodeset="/data/repeat/child/group_other_woman_pregnancy/fp_flow/fp" required="true()" type="select1"/>
      <bind constraint="not(selected(., 'fp_none') and count-selected(.)&gt;=2) and count-selected(.)&lt;=2 and not(count-selected(.)=2 and not(selected(., 'condoms') ))" jr:constraintMsg="None cannot be selected along with the other options. 
Condoms are the only family planing method that can be combined with another family planning method" nodeset="/data/repeat/child/group_other_woman_pregnancy/fp_flow/family_planning_method" relevant="selected( /data/repeat/child/group_other_woman_pregnancy/fp_flow/fp , 'true')" required="true()" type="select"/>
      <bind nodeset="/data/repeat/child/group_other_woman_pregnancy/fp_flow/fp_enroll" relevant="selected( /data/repeat/child/group_other_woman_pregnancy/fp_flow/fp , 'false') or selected( /data/repeat/child/group_other_woman_pregnancy/fp_flow/family_planning_method , 'fp_none')" required="true()" type="select1"/>
      <bind constraint="count-selected(.)&lt;=2 and not(count-selected(.)=2 and not(selected(., 'condoms') ))" jr:constraintMsg="Condoms are the only family planing method that can be combined with another family planning method" nodeset="/data/repeat/child/group_other_woman_pregnancy/fp_flow/fp_method_choice" relevant="selected( /data/repeat/child/group_other_woman_pregnancy/fp_flow/fp_enroll , 'true')" required="true()" type="select"/>
      <bind nodeset="/data/repeat/child/group_other_woman_pregnancy/fp_flow/note_fp_1" readonly="true()" relevant="selected( /data/repeat/child/group_other_woman_pregnancy/fp_flow/fp_enroll , 'true') and not(selected( /data/repeat/child/group_other_woman_pregnancy/fp_flow/fp_method_choice , 'iud') or selected( /data/repeat/child/group_other_woman_pregnancy/fp_flow/fp_method_choice , 'btl') or selected( /data/repeat/child/group_other_woman_pregnancy/fp_flow/fp_method_choice , 'norplant'))" type="string"/>
      <bind nodeset="/data/repeat/child/group_other_woman_pregnancy/fp_flow/note_fp_2" readonly="true()" relevant="selected( /data/repeat/child/group_other_woman_pregnancy/fp_flow/fp_enroll , 'true') and (selected( /data/repeat/child/group_other_woman_pregnancy/fp_flow/fp_method_choice , 'iud') or selected( /data/repeat/child/group_other_woman_pregnancy/fp_flow/fp_method_choice , 'btl') or selected( /data/repeat/child/group_other_woman_pregnancy/fp_flow/fp_method_choice , 'norplant'))" type="string"/>
      <bind nodeset="/data/repeat/child/group_other_woman_pregnancy/fp_flow/note_fp_3" readonly="true()" relevant="selected( /data/repeat/child/group_other_woman_pregnancy/fp_flow/fp , 'false') and selected( /data/repeat/child/group_other_woman_pregnancy/fp_flow/fp_enroll , 'false')" type="string"/>
      <bind calculate="/data/inputs/user/facility_id" nodeset="/data/clinic/parent" type="string"/>
      <bind calculate="concat(../../contact/name, ' Family')" nodeset="/data/clinic/name" type="string"/>
      <bind calculate="concat(../../inputs/meta/location/lat, concat(' ', ../../inputs/meta/location/long))" nodeset="/data/clinic/geolocation" type="string"/>
      <bind calculate="&quot;NEW&quot;" nodeset="/data/clinic/contact" type="string"/>
      <bind nodeset="/data/clinic/household_survey/safe_water" type="select1"/>
      <bind nodeset="/data/clinic/household_survey/source_clean_water" relevant=" /data/clinic/household_survey/safe_water ='true'" type="select"/>
      <bind nodeset="/data/clinic/household_survey/source_of_drinking_water" relevant=" /data/clinic/household_survey/safe_water ='false'" required="true()" type="select"/>
      <bind nodeset="/data/clinic/household_survey/kitchen" required="true()" type="select1"/>
      <bind nodeset="/data/clinic/household_survey/drying_rack" required="true()" type="select1"/>
      <bind nodeset="/data/clinic/household_survey/rubbish_pit" required="true()" type="select1"/>
      <bind nodeset="/data/clinic/household_survey/hygeinic_toilet" required="true()" type="select1"/>
      <bind nodeset="/data/clinic/household_survey/mosquito_nets" required="true()" type="select1"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/data/meta/instanceID" readonly="true()" type="string"/>
    </model>
  </h:head>
  <h:body class="pages">
    <group ref="/data/inputs">
      <group ref="/data/inputs/user">
        <input ref="/data/inputs/user/facility_id">
          <label>Facility ID for the parent user</label>
        </input>
      </group>
    </group>
    <group appearance="field-list" ref="/data/contact">
      <label>Primary Caregiver</label>
      <input ref="/data/contact/debug_note">
        <label>Debug: <output value=" /data/inputs/user/facility_id "/></label>
      </input>
      <input appearance="hidden" ref="/data/contact/parent">
        <label>Parent</label>
        <hint>mandatory</hint>
      </input>
      <input ref="/data/contact/care_giver">
        <label>Primary Caregiver</label>
      </input>
      <input appearance="numbers" ref="/data/contact/phone1">
        <label>Primary Phone Number</label>
      </input>
    </group>
    <group appearance="field-list" ref="/data/ephemeral_dob">
      <label>Primary Caregiver</label>
      <select1 appearance="horizontal" ref="/data/ephemeral_dob/g_sex">
        <label>Sex</label>
        <item>
          <label>Female</label>
          <value>female</value>
        </item>
        <item>
          <label>Male</label>
          <value>male</value>
        </item>
      </select1>
      <select1 appearance="horizontal hidden" ref="/data/ephemeral_dob/contact_dob_method">
        <label>Method to select date of birth</label>
        <item>
          <label>Date (preferred)</label>
          <value>calendar</value>
        </item>
        <item>
          <label>Current age</label>
          <value>approx</value>
        </item>
      </select1>
      <input ref="/data/ephemeral_dob/dob_calendar">
        <label>Date of Birth</label>
      </input>
      <input ref="/data/ephemeral_dob/age">
        <label>Age</label>
      </input>
    </group>
    <group appearance="field-list" ref="/data/ephemeral_pregnancy">
      <label>Primary Caregiver</label>
      <select1 appearance="horizontal" ref="/data/ephemeral_pregnancy/pregnant">
        <label>Is <output value=" /data/ephemeral_pregnancy/display_name "/> currently pregnant?</label>
        <item>
          <label>Yes</label>
          <value>true</value>
        </item>
        <item>
          <label>No</label>
          <value>false</value>
        </item>
      </select1>
      <input ref="/data/ephemeral_pregnancy/note_p_form">
        <label>Please fill out a Pregnancy Registration for the Primary Caregiver</label>
      </input>
      <select1 appearance="horizontal" ref="/data/ephemeral_pregnancy/lmp">
        <label>Does she know the date of the last menstrual cycle?</label>
        <item>
          <label>Yes</label>
          <value>true</value>
        </item>
        <item>
          <label>No</label>
          <value>false</value>
        </item>
      </select1>
      <input ref="/data/ephemeral_pregnancy/lmp_start">
        <label>Start date of the last cycle</label>
      </input>
      <select1 ref="/data/ephemeral_pregnancy/lmp_approx">
        <label>Approximate date of last cycle</label>
        <item>
          <label>Up to 2 months</label>
          <value>2mo</value>
        </item>
        <item>
          <label>Up to 3 months</label>
          <value>3mo</value>
